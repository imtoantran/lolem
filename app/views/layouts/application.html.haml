                          %ul.dropdown-menu
                            %li
                              %a{:href => "index-blog.html"}
                                Blog Home
                                %span.badge New
                            %li.dropdown
                              %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"} Large Image
                              %ul.dropdown-menu.to-left
