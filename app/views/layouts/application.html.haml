                                  %a{:href => "blog-large-image-right-sidebar.html"} Right Sidebar
                                %li
                                  %a{:href => "blog-large-image-left-sidebar.html"} Left Sidebar
                                %li
                                  %a{:href => "blog-large-image-no-sidebar.html"} Without Sidebar
                            %li.dropdown
                              %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"} Medium Image
                              %ul.dropdown-menu.to-left
                                %li
                                  %a{:href => "blog-medium-image-right-sidebar.html"} Right Sidebar
                                %li
                                  %a{:href => "blog-medium-image-left-sidebar.html"} Left Sidebar
                                %li
                                  %a{:href => "blog-medium-image-no-sidebar.html"} Without Sidebar
                            %li.dropdown
                              %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"} Masonry
                              %ul.dropdown-menu.to-left
                                %li
                                  %a{:href => "blog-masonry-right-sidebar.html"} Right Sidebar
                                %li
                                  %a{:href => "blog-masonry-left-sidebar.html"} Left Sidebar
                                %li
                                  %a{:href => "blog-masonry-no-sidebar.html"} Without Sidebar
                            %li
                              %a{:href => "blog-timeline.html"} Timeline
